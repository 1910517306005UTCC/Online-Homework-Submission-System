<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.utcc.onlinehomeworksubmissionsystem"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="30" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.OnlineHomeworkSubmissionSystem" >
        <activity android:name="com.utcc.onlinehomeworksubmissionsystem.MainStudent" />
        <activity android:name="com.utcc.onlinehomeworksubmissionsystem.JobInAssignment" />
        <activity android:name="com.utcc.onlinehomeworksubmissionsystem.AssignWork" />
        <activity android:name="com.utcc.onlinehomeworksubmissionsystem.Login" />
        <activity android:name="com.utcc.onlinehomeworksubmissionsystem.Register" />
        <activity
            android:name="com.utcc.onlinehomeworksubmissionsystem.ui.login.RegisterActivity"
            android:label="@string/title_activity_register" />
        <activity android:name="com.utcc.onlinehomeworksubmissionsystem.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>